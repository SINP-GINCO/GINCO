{# Denombrement #}
<cont:denombrement>
    <cont:DenombrementType>
        {% if denombrement.typedenombrement %}
            <cont:typeDenombrement>{{ denombrement.typedenombrement }}</cont:typeDenombrement>
        {% endif %}
        <cont:objetDenombrement>{{ denombrement.objetdenombrement }}</cont:objetDenombrement>
        <cont:denombrementMax>{{ denombrement.denombrementmax }}</cont:denombrementMax>
        <cont:denombrementMin>{{ denombrement.denombrementmin }}</cont:denombrementMin>
    </cont:DenombrementType>
</cont:denombrement>