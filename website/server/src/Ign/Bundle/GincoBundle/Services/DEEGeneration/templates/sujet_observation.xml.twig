<gml:featureMember>
    <cont:SujetObservation gml:id="ID#GMLID#">
        {% if (observation.descriptifsujet) %}
            {% include 'descriptif_sujet.xml.twig' with {'descriptifsujet': observation.descriptifsujet} %}
        {% endif %}
        {#Â Todo : AttributAdditionnel #}
        {% for departement in observation.departements %}
            {% include 'departement.xml.twig' %}
        {% endfor %}
        {% for me in observation.massesdeau %}
            {% include 'masse_d_eau.xml.twig' %}
        {% endfor %}
        {% for maille in observation.mailles %}
            {% include 'maille.xml.twig' %}
        {% endfor %}
        {% for commune in observation.communes %}
            {% include 'commune.xml.twig' %}
        {% endfor %}
        {% if (observation.source) %}
            <cont:EstSourcePar>
                {% include 'source.xml.twig' with {'source': observation.source} %}
            </cont:EstSourcePar>
        {% endif %}
        {% for en in observation.espacesnaturels %}
            {% include 'espace_naturel.xml.twig' %}
        {% endfor %}
        {% if observation.identifiantregroupementpermanent %}
            <cont:identifiantRegroupementParent>{{ observation.identifiantregroupementpermanent }}</cont:identifiantRegroupementParent>
        {% endif %}
        {% if observation.commentaire %}
            <cont:commentaire>{{ observation.commentaire }}</cont:commentaire>
        {% endif %}
        {% if observation.organismestandard %}
            <cont:organismeStandard>
                <cont:OrganismeType>
                    <cont:nomOrganisme>{{ observation.organismestandard }}</cont:nomOrganisme>
                </cont:OrganismeType>
            </cont:organismeStandard>
        {% endif %}
        {% if observation.validateur %}
            <cont:validateur>
                {% include 'personne.xml.twig' with {'personne': observation.validateur} %}
            </cont:validateur>
        {% endif %}
        {% if observation.datedetermination %}
            <cont:dateDetermination>{{ observation.datedetermination }}</cont:dateDetermination>
        {% endif %}
        {% if observation.determinateur %}
            <cont:determinateur>
                {% include 'personne.xml.twig' with {'personne': observation.determinateur} %}
            </cont:determinateur>
        {% endif %}
        {% if observation.versiontaxref %}
            <cont:versionTAXREF>{{ observation.versiontaxref }}</cont:versionTAXREF>
        {% endif %}
        {% if observation.cdref %}
            <cont:cdRef>{{ observation.cdref }}</cont:cdRef>
        {% endif %}
        {% if observation.cdnom %}
            <cont:cdNom>{{ observation.cdnom }}</cont:cdNom>
        {% endif %}
        {% if observation.profondeurmoyenne %}
            <cont:profondeurMoyenne>{{ observation.profondeurmoyenne }}</cont:profondeurMoyenne>
        {% endif %}
        <cont:observateur>
            {% include 'personne.xml.twig' with {'personne': observation.observateur} %}
        </cont:observateur>
        {% if observation.profondeurmin %}
            <cont:profondeurMin>{{ observation.profondeurmin }}</cont:profondeurMin>
        {% endif %}
        {% if observation.profondeurmax %}
            <cont:profondeurMax>{{ observation.profondeurmax }}</cont:profondeurMax>
        {% endif %}
        {% if (observation.denombrement) %}
            {% include 'denombrement.xml.twig' with {'denombrement': observation.denombrement} %}
        {% endif %}
        {% for habitat in observation.habitats %}
            {% include 'habitat.xml.twig' %}
        {% endfor %}
        {% if observation.altitudemax %}
            <cont:altitudeMax>{{ observation.altitudemax }}</cont:altitudeMax>
        {% endif %}
        {% if observation.altitudemoyenne %}
            <cont:altitudeMoyenne>{{ observation.altitudemoyenne }}</cont:altitudeMoyenne>
        {% endif %}
        {% if observation.altitudemin %}
            <cont:altitudeMin>{{ observation.altitudemin }}</cont:altitudeMin>
        {% endif %}
        <cont:dateFin>{{ observation.datefin }}</cont:dateFin>
        <cont:dateDebut>{{ observation.datedebut }}</cont:dateDebut>
        {% if (observation.objetgeo) %}
            {% include 'objet_geo.xml.twig' with {'objetgeo': observation.objetgeo} %}
        {% endif %}
        <cont:nomCite>{{ observation.nomcite }}</cont:nomCite>
        <cont:statutObservation>{{ observation.statutobservation }}</cont:statutObservation>
        <cont:identifiantPermanent>{{ observation.identifiantpermanent }}</cont:identifiantPermanent>
    </cont:SujetObservation>
</gml:featureMember>
