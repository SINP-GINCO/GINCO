<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Metadata.process" table="metadata.process">
    <id name="processId" type="string" column="process_id" length="50">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="step" type="string" column="step" length="50" nullable="true"/>
    <field name="label" type="string" column="label" length="60" nullable="true"/>
    <field name="description" type="string" column="description" length="500" nullable="true"/>
    <field name="statement" type="string" column="statement" length="4000" nullable="true"/>
    <field name="creationdt" type="datetime" column="_creationdt" nullable="true"/>
  </entity>
</doctrine-mapping>
